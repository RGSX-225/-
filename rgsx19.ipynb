fid=open("bjyx.txt","r")
#read()直接读取 readline()读取一条数据  readlines()读取所有数据（但换行被存为换行符\n） for按行读取 with open 自动关闭
typenum=[]
item={}
for f in fid:
    for i in f:
        if i in typenum:
            item[i]+=1
        else:
            typenum.append(i)
            item[i]=1
item_sort=sorted(item.items(),key=lambda d:d[1],reverse=True)  #按个数对字典排序
print(item_sort)
for i in range(20):
    print(item_sort[i])     #输出排名前二十的字符及次数
